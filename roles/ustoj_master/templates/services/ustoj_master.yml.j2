apiVersion: v1
kind: Service
metadata:
  name: ustoj-master-service
spec:
  selector:
    app: ustoj-master
  ports:
    - protocol: TCP
      port: 80
      targetPort: 80
---
apiVersion: v1
kind: Service
metadata:
  name: ustoj-master-ticker-service
spec:
  selector:
    app: ustoj-master-ticker
  ports:
    - protocol: TCP
      port: 80
      targetPort: 80